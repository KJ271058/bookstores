<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cdtvc.book.vo.BookEntity" table="book" schema="book station">
        <id name="bookId" column="Book_ID"/>
        <property name="typeId" column="Type_ID"/>
        <property name="bookName" column="Book_name"/>
        <property name="bookData" column="Book_data"/>
        <property name="bookPrice" column="Book_price"/>
    </class>
</hibernate-mapping>